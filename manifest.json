{
    "name": "YouTube Scroll",
    "description": "Navigate with Scroll",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "permissions": [
      "storage"
    ],
    "options_page": "options.html"
}
